{
  "buildCommand": "npm run build",
  "outputDirectory": "dillydally-frontend/dist",
  "devCommand": "npm run dev --workspace=dillydally-frontend",
  "installCommand": "npm install && npm install --workspaces --include-workspace-root",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

