{
  "name": "dillydally-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-convex": "cd ../.. && npx convex codegen && cd dillydally-frontend",
    "copy-convex": "node scripts/copy-convex-generated.js",
    "dev": "npm run copy-convex && vite",
    "build": "sh -c '[ -n \"$VERCEL\" ] && npm install --include=optional @rollup/rollup-linux-x64-gnu || true' && npm run generate-convex && npm run copy-convex && tsc -b && vite build",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite preview"
  },
  "dependencies": {
    "convex": "^1.28.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
