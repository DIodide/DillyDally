    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "convex": "^1.28.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-webcam": "^7.2.0",
    "@auth/core": "^0.37.0",
    "@convex-dev/auth": "^0.0.90"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
